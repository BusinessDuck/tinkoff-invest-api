{
  "name": "tinkoff-invest-api",
  "description": "Node.js SDK for Tinkoff Invest API",
  "version": "4.0.0-0",
  "type": "module",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./cjs/index.js"
    },
    "./common": {
      "import": "./dist/generated/common.js",
      "require": "./cjs/generated/common.js"
    },
    "./instruments": {
      "import": "./dist/generated/instruments.js",
      "require": "./cjs/generated/instruments.js"
    },
    "./marketdata": {
      "import": "./dist/generated/marketdata.js",
      "require": "./cjs/generated/marketdata.js"
    },
    "./operations": {
      "import": "./dist/generated/operations.js",
      "require": "./cjs/generated/operations.js"
    },
    "./orders": {
      "import": "./dist/generated/orders.js",
      "require": "./cjs/generated/orders.js"
    },
    "./sandbox": {
      "import": "./dist/generated/sandbox.js",
      "require": "./cjs/generated/sandbox.js"
    },
    "./stoporders": {
      "import": "./dist/generated/stoporders.js",
      "require": "./cjs/generated/stoporders.js"
    },
    "./users": {
      "import": "./dist/generated/users.js",
      "require": "./cjs/generated/users.js"
    }
  },
  "files": [
    "src",
    "dist",
    "cjs",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "tsc": "tsc --noEmit",
    "check": "ts-node-esm scripts/check.ts",
    "check:d": "DEBUG=tinkoff-invest-api:* ts-node-esm scripts/check.ts",
    "test": "mocha -n loader=ts-node/esm test/setup.ts test/specs/*.ts -t 7000",
    "test:d": "DEBUG=tinkoff-invest-api:* npm t",
    "test:modules": "node test/modules/test.cjs && node test/modules/test.mjs",
    "pull-proto": "./scripts/pull-proto.sh",
    "compile-proto": "./scripts/compile-proto.sh",
    "toc": "markdown-toc README.md -i",
    "build:esm": "rm -rf dist && tsc -p tsconfig.build.json",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.build.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
    "build": "npm run build:esm && npm run build:cjs",
    "release": "npm run lint && npm t && npm run build && npm run test:modules && np --yolo --no-release-draft"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "long": "^4.0.0",
    "ms": "^3.0.0-beta.2",
    "nice-grpc": "^1.2.0",
    "protobufjs": "^6.11.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-mocha": "^10.0.5",
    "fast-glob": "^3.2.11",
    "lint-staged": "^13.0.1",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.0.0",
    "np": "^7.6.0",
    "ts-node": "^10.8.1",
    "ts-proto": "^1.115.4",
    "typescript": "^4.7.3"
  },
  "author": "Vitaliy Potapov",
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/tinkoff-invest-api.git"
  },
  "keywords": [
    "tinkoff",
    "invest",
    "api"
  ],
  "license": "MIT"
}
