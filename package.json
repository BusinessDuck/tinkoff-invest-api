{
  "name": "tinkoff-invest-api",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "tsc": "tsc --noEmit",
    "check": "ts-node-esm scripts/check.ts",
    "test": "mocha -n loader=ts-node/esm test/setup.ts test/specs/*.ts",
    "pull-proto": "./scripts/pull-proto.sh",
    "compile-proto": "./scripts/compile-proto.sh",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "release": "npm run lint && npm t && npm run build && np --yolo --no-release-draft"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "long": "^4.0.0",
    "nice-grpc": "^1.1.1",
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-mocha": "^10.0.3",
    "lint-staged": "^12.3.3",
    "mocha": "^9.2.0",
    "np": "^7.6.0",
    "ts-node": "^10.7.0",
    "ts-proto": "^1.110.4",
    "typescript": "^4.6.3"
  },
  "author": "Vitaliy Potapov",
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/tinkoff-invest-api.git"
  },
  "license": "MIT"
}
